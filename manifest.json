{
  "manifest_version": 2,
  "name": "PodBuddy",
  "version": "2.0",
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*",
		"https://stackoverflow.com/*"
      ],
       "js": ["jquery.js","content.js"]
       
    }
  ],
  "browser_action":{
  "default_title":"PodBuddy",
  "default_popup":"popup.html",
  "default_icon":"icon.png"
  },
  "permissions": [
   "activeTab"
   ]
}